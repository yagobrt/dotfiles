# Shortcuts for some directories

map gv  cd ~/Vídeos/
map gdl cd ~/Descargas/
map gdc cd ~/Documentos/
map gi  cd ~/Imágenes/
map ge  cd ~/Escritorio/
map gmu cd ~/Música/
map gmt cd /media/yago/

map gj  cd ~/Documentos/japanese/
map gb  cd ~/Documentos/books/
map gn  cd ~/Documentos/obsidian-notes/
map gc  cd ~/Documentos/dotfiles/
map gr  cd ~/Documentos/reference/
map gu  cd ~/Documentos/uni/
map gt cd ~/Documentos/uni/tfg/
map gf cd ~/Documentos/uni/tfg/

# Go to trash
map gT cd ~/.local/share/Trash/files/
